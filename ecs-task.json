{
    "family": "archive-analyzer",
    "containerDefinitions": [
        {
            "name": "archive-analyzer",
            "image": "YOUR_AWS_ACCOUNT_ID.dkr.ecr.YOUR_AWS_REGION.amazonaws.com/archive-analyzer",
            "portMappings": [
                {
                    "containerPort": 7860,
                    "hostPort": 7860
                }
            ],
            "memory": 512,
            "cpu": 256,
            "environment": [
                {
                    "name": "OPENAI_API_KEY",
                    "value": "your-openai-api-key"
                },
                {
                    "name": "HUGGINGFACE_API_KEY",
                    "value": "your-huggingface-api-key"
                }
            ],
            "resourceRequirements": [
                {
                    "type": "MEMORY",
                    "value": "512"
                },
                {
                    "type": "CPU",
                    "value": "256"
                }
            ]
        }
    ]
}
